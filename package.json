{
  "name": "my-rm-dir",
  "version": "1.3.1",
  "type": "module",
  "private": false,
  "description": "Quickly delete large folders/files, such as `node_modules` with thousands of small files",
  "keywords": [
    "my-rm-dir",
    "rm-dir",
    "rimraf",
    "delete",
    "node_modules"
  ],
  "author": "xxxxue <1659809758@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/xxxxue/my-rm-dir#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxxxue/my-rm-dir.git"
  },
  "bugs": {
    "url": "https://github.com/xxxxue/my-rm-dir/issues"
  },
  "files": [
    "src"
  ],
  "bin": {
    "my-rm-dir": "/src/main.js"
  },
  "preferGlobal": true,
  "main": "/src/main.js",
  "module": "./src/main.js",
  "scripts": {
    "npm-login": "npm login --registry=https://registry.npmjs.org",
    "npm-publish": "npm publish --registry=https://registry.npmjs.org",
    "npm-remove-version": "npm unpublish my-rm-dir@1.0.0 --force --registry=https://registry.npmjs.org",
    "dev-link": "npm link",
    "dev-unlink": "npm uninstall -g my-rm-dir"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/rimraf": "^3.0.2"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.3.1",
    "chalk": "^5.4.1",
    "press-any-key": "^1.1.0",
    "rimraf": "^3.0.2",
    "round-to": "^6.0.0"
  }
}